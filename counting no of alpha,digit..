x=input("enter a string")
u=0
l=0
d=0
a=0
for i in x:
    if(i.isdigit()):
        d=d+1
    elif (i.isalpha()):
        a=a+1 
        if (i.islower()):
           l=l+1
        elif (i.isupper()):
           u=u+1
    else:
        continue
print("no of digits:",d)
print("no of alphabets:",a)
print("no of lower case letters:",l)
print("no of upper case letters:",u)
